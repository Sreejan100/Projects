<div id='newbuses'>
    <div class='container'>
        <h1 style="color:#ffa07a;">Your Ticket</h1>
        <div class='about-wrapper newzealand'>
            <table class="table table-striped">
             <thead>
               <tr>
                <th>busno</th>
                <th>Name</th>
                <th>Source</th>
                <th>Destination</th>
                <th>Journey Date</th>
                <th>Departure Time</th>
                <th>Age</th>
                <th>Number of Seats</th>
              </tr>
           </thead>
            <tbody>
                <% articles.forEach(function(article){ %>
                <tr>
                    <td><%= article.busno %></td>
                    <td><%= article.name %></td>
                    <td><%= article.source %></td>
                    <td><%= article.destination %></td>
                    <td><%= article.journey_date %></td>
                    <td><%= article.departure_time %></td>
                    <td><%= article.age %></td>
                    <td><%= article.no_of_seats %></td>
                </tr>
            <% }) %>
