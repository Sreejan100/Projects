<div id='americabus'>
    <div class="container">
        <h1 id="fd" style="color:#FFA07A;">BusyBus(America)</h1>
        <div class="about-wrapper america">
            
         <table class="table table-striped america">
            <thead>
              <tr>
                <th>busno</th>
                <th>Country</th>
                <th>Source</th>
                <th>Destination</th>
                <th>Date</th>
                <th>Time</th>
                <th>Number of Seats</th>
              </tr>
           </thead>
           <tbody>

            <% articles.forEach(function(article){ %>
                <tr>
                    <td><%= article.busno %></td>
                    <td><%= article.country %></td>
                    <td><%= article.source %></td>
                    <td><%= article.destination %></td>
                    <td><%= article.date %></td>
                    <td><%= article.time %></td>
                    <td><%= article.no_of_seats %></td>
                </tr>
            <% }) %>
